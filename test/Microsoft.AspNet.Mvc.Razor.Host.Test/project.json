{
    "resource": "TestFiles/**",
    "dependencies": {
        "Microsoft.AspNet.Mvc.Core": "6.0.0-beta8",
        "Microsoft.AspNet.Mvc.DataAnnotations": "6.0.0-beta8",
        "Microsoft.AspNet.Mvc.Formatters.Xml": "6.0.0-beta8",
        "Microsoft.AspNet.Mvc.Razor": "6.0.0-beta8",
        "Microsoft.AspNet.Mvc.Razor.Host": "6.0.0-beta8",
        "Microsoft.AspNet.Mvc.ViewFeatures": "6.0.0-beta8",
        "Microsoft.AspNet.Mvc.TestCommon": {
            "version": "6.0.0-*",
            "type": "build"
        },
        "Microsoft.AspNet.Testing": "1.0.0-beta8",
        "Microsoft.Framework.WebEncoders.Testing": "1.0.0-beta8",
        "xunit.runner.aspnet": "2.0.0-aspnet-beta8"
    },
    "commands": {
        "test": "xunit.runner.aspnet"
    },
    "compilationOptions": {
        "define": [
            "__RemoveThisBitTo__GENERATE_BASELINES"
        ]
    },
    "frameworks": {
        "dnx451": {
            "dependencies": {
                "Moq": "4.2.1312.1622"
            }
        }
    },
    "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components"
    ]
}
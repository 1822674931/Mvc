{
    "description": "Design time hosting infrastructure for the ASP.NET MVC Razor view engine.",
    "version": "6.0.0-beta6",
    "repository": {
        "type": "git",
        "url": "git://github.com/aspnet/mvc"
    },
    "compilationOptions": {
        "warningsAsErrors": true
    },
    "dependencies": {
        "Microsoft.AspNet.FileProviders.Physical": "1.0.0-beta6",
        "Microsoft.AspNet.Razor.Runtime": "4.0.0-beta6",
        "Microsoft.Framework.Caching.Memory": "1.0.0-beta6",
        "Microsoft.Framework.PropertyHelper.Sources": {
            "version": "1.0.0-beta6",
            "type": "build"
        },
        "Microsoft.Framework.NotNullAttribute.Sources": {
            "version": "1.0.0-beta6",
            "type": "build"
        }
    },
    "frameworks": {
        "net45": {},
        "dnx451": {
            "dependencies": {
                "Microsoft.Framework.Runtime.Abstractions": "1.0.0-beta6"
            }
        },
        "dnxcore50": {
            "dependencies": {
                "Microsoft.Framework.Runtime.Abstractions": "1.0.0-beta6",
                "System.Collections.Concurrent": "4.0.10-beta-23109",
                "System.ComponentModel.TypeConverter": "4.0.0-beta-23109",
                "System.Reflection.Extensions": "4.0.0-beta-23109",
                "System.Text.Encoding.Extensions": "4.0.10-beta-23109"
            }
        }
    },
    "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components"
    ],
    "packExclude": [
        "node_modules",
        "bower_components",
        "**.kproj",
        "**.user",
        "**.vspscc"
    ]
}
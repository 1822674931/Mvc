{
    "description": "Metadata and Validation using System.ComponentModel.DataAnnotations for the MVC runtime",
    "version": "6.0.0-beta8",
    "repository": {
        "type": "git",
        "url": "git://github.com/aspnet/mvc"
    },
    "compilationOptions": {
        "warningsAsErrors": true
    },
    "dependencies": {
        "Microsoft.AspNet.Mvc.Core": "6.0.0-beta8",
        "Microsoft.Framework.ClosedGenericMatcher.Sources": {
            "version": "1.0.0-beta8",
            "type": "build"
        },
        "Microsoft.Framework.CopyOnWriteDictionary.Sources": {
            "version": "1.0.0-beta8",
            "type": "build"
        },
        "Microsoft.Framework.Localization": "1.0.0-beta8"
    },
    "frameworks": {
        "dnx451": {
            "frameworkAssemblies": {
                "System.ComponentModel.DataAnnotations": ""
            }
        },
        "dnxcore50": {
            "dependencies": {
                "System.ComponentModel.Annotations": "4.0.11-beta-23409"
            }
        }
    }
}